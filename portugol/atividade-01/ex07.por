programa {
	funcao inicio() {
		real a = 0
		real b = 0
		real c = 0
		real d = 0
		real ee = 0
		real f = 0
		real x = 0
		real y = 0
		limpa()
		
		a = 1
		b = 2
		c = 3
		d = 4
		ee = 5
		f = 6
		
		x = ((c*ee) - (b*f)) / ((a*ee) - (b*d))
		y = ((a*f) - (c*d)) / ((a*ee) - (b*d))
		
		escreva("\nResultados : \nx ", x, "\ny ", y)
		
	}
}
